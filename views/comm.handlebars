<div class="container">
  <div class="main main-raised top-margin bottom-margin">
    <div class="row">
      <div class="col-lg-12">
        <div class="row">
          <div class="col-lg-3 mt-3 mr-3 ml-3 mb-3 text-center">
            <img class="mb-3 rounded-circle border border-purple" src="{{comm.photoUrl}}" style="width: 100%; border-width: 4px !important;">
            <a href="#" id="join-community" data-id="{{id}}" class="btn btn-primary btn-link btn-wd btn-lg">Join Community</a>
          </div>
          <div class="col-lg-8 mt-3 mr-3 ml-3 mb-3">
            <h2>{{comm.name}}<small class="ml-2"><small>{{comm.location}}</small></small></h2>
            <p>{{comm.bio}}</p>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6 ml-auto mr-auto">
        <div class="profile-tabs mb-3">
          <ul class="nav nav-pills nav-pills-icons justify-content-center" role="tablist">
            <li class="nav-item">
              <a class="nav-link" href="#members" role="tab" data-toggle="tab">
                <i class="material-icons">group</i> Members
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" href="#message-board" role="tab" data-toggle="tab">
                <i class="material-icons">message</i> Message Board
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#photos" role="tab" data-toggle="tab">
                <i class="material-icons">photo_library</i> Photos
              </a>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <div class="tab-content tab-space">
      <div class="tab-pane text-center gallery col-lg-10 mx-auto" id="members">
        <div class="row">
          <div id="members-container">
            <ul class="nav nav-pills nav-pills-icons justify-content-center" role="tablist">
              {{#each comm.Communities}}
                {{> userCommunities/communityMember-block}}
              {{/each}}
            </ul>
          </div>
        </div>
      </div>
      <div class="tab-pane active text-center gallery" id="message-board">
        <div class="row">
          <div class="col-lg-10 mx-auto remove-mt">
            <div class="card border border-purple rounded">
              <div class="card-body">
                <div class="md-form form-group text-left remove-mt">
                  <label for="form10" class="bmd-label-floating">Write a new message...</label>
                  <textarea type="text" id="form10" class="md-textarea form-control" rows="3"></textarea>
                  <button type="submit" class="btn btn-light btn-fab btn-raised btn-round">
                    <i class="material-icons">insert_photo</i>
                  </button>
                  <button type="submit" class="btn btn-light btn-fab btn-raised btn-round">
                    <i class="material-icons">insert_drive_file</i>
                  </button>
                  <button type="submit" class="btn btn-light btn-fab btn-raised btn-round">
                    <i class="material-icons">insert_invitation</i>
                  </button>
                  <button type="submit" class="btn btn-light btn-fab btn-raised btn-round">
                    <i class="material-icons">insert_emoticon</i>
                  </button>
                  <button type="submit" id="send-message" class="btn btn-primary btn-raised btn-round float-right">
                    <i class="material-icons mr-1">insert_comment</i> Submit
                  </button>
                </div>
                <ul class="list-group list-group-flush">
                  <div id="message-container">
                  </div>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="tab-pane text-center gallery" id="photos">
        <div class="row">
          <div class="col-lg-10 mx-auto">
            <div class="row">
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://66.media.tumblr.com/45a28fbddc5b75da42ddc8674002399f/tumblr_osip4zQBfe1qh6pbfo1_1280.jpg" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/2d36e011323a4a3dcbb17bef942ba2bc.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/05863dfeaa07e2512e43f05a6410cf88.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/029cfbdf0b5edc1d9ad73aec40713fe5.jpg" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/9417df045186cade42d801ee0162d337.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/ac3172e2d2053bdc4f76f1ad7e5423c2.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/53bbfe113fbd7e8121aeab5b02f5c280.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/2b9dfb9eb6e31690f1dc4f763a37883a.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/1b9115974b4ca391c88d87568cbad7cc.jpg" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/a81c1daeec39faca9f9f2536848f1748.jpg" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/ce287212562dd9fc571254fb676e94cb.png" style="width: 100%">
              </div>
              <div class="col-md-3">
                <img class="rounded border border-3 border-purple mb-3" src="https://i.gyazo.com/443c80a5548633b65c03a4219a5813a2.png" style="width: 100%">
              </div>
            </div>
          </div>
          <div class="fix-to-bottom">
            <a href="#pablo" class="btn btn-primary btn-link btn-wd btn-lg">see all...</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>